  overflow: hidden;
  height:60rpx;
}
.imageView{
  width:750rpx;
  text-align: center;
  position: relative;
  background-color: #cc9999;
  overflow: hidden;
}
.noImageBox{
  width:100%;
  height:100%;
  text-align: center;
  line-height: 1000rpx;
}
.saveButton{
  position:absolute;
  top:30rpx;
  right:40rpx;
  padding: 10rpx;
  font-size: 0;
  opacity: 0.7;
  z-index: 5;
}
